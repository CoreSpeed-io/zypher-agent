{
  "name": "@zypher/server",
  "version": "1.0.0",
  "exports": {},
  "tasks": {
    "start": "deno run -A src/index.ts",
    "dev": "deno run --watch -A src/index.ts",
    "build": "deno compile --allow-all --frozen=true --output dist/server src/index.ts",
    "bundle": "rolldown -c rolldown.config.mts"
  },
  "imports": {
    "@deno/rolldown-plugin": "jsr:@deno/rolldown-plugin@^0.0.7",
    "@zypher/core": "../core/mod.ts",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.5",
    "@std/cli": "jsr:@std/cli@^1.0.20",
    "hono": "npm:hono@^4.7.11",
    "@hono/zod-validator": "npm:@hono/zod-validator@^0.7.0",
    "rolldown": "npm:rolldown@^1.0.0-beta.18",
    "rxjs": "npm:rxjs@^7.8.2",
    "rxjs-for-await": "npm:rxjs-for-await@^1.0.0",
    "zod": "npm:zod@^3.25.64"
  }
}
