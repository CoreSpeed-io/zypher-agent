{
  "publish": false,
  "tasks": {
    "dev": "vite",
    "build": "deno check . && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "imports": {
    "@/": "./src/",
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.6",
    "@eslint/js": "npm:@eslint/js@^9.39.2",
    "@radix-ui/react-collapsible": "npm:@radix-ui/react-collapsible@^1.1.12",
    "@radix-ui/react-slot": "npm:@radix-ui/react-slot@^1.2.4",
    "@radix-ui/react-tooltip": "npm:@radix-ui/react-tooltip@^1.2.8",
    "@radix-ui/react-use-controllable-state": "npm:@radix-ui/react-use-controllable-state@^1.2.2",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.1.18",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^5.1.2",
    "class-variance-authority": "npm:class-variance-authority@^0.7.1",
    "clsx": "npm:clsx@^2.1.1",
    "eslint": "npm:eslint@^9.39.2",
    "foxact": "npm:foxact@^0.2.50",
    "eslint-plugin-react-hooks": "npm:eslint-plugin-react-hooks@^7.0.1",
    "eslint-plugin-react-refresh": "npm:eslint-plugin-react-refresh@^0.4.26",
    "globals": "npm:globals@^17.2.0",
    "lucide-react": "npm:lucide-react@^0.563.0",
    "radix-ui": "npm:radix-ui@^1.4.3",
    "react": "npm:react@^19.2.4",
    "react-dom": "npm:react-dom@^19.2.4",
    "tailwind-merge": "npm:tailwind-merge@^3.4.0",
    "tailwindcss": "npm:tailwindcss@^4.1.18",
    "typescript-eslint": "npm:typescript-eslint@^8.54.0",
    "use-stick-to-bottom": "npm:use-stick-to-bottom@^1.1.2",
    "streamdown": "npm:streamdown@^2.1.0",
    "@streamdown/cjk": "npm:@streamdown/cjk@^1.0.1",
    "@streamdown/code": "npm:@streamdown/code@^1.0.1",
    "@streamdown/math": "npm:@streamdown/math@^1.0.1",
    "@streamdown/mermaid": "npm:@streamdown/mermaid@^1.0.1",
    "vite": "npm:vite@^7.3.1"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "strict": true,
    "lib": [
      "dom",
      "dom.iterable",
      "dom.asynciterable",
      "deno.ns",
      "deno.unstable"
    ],
    "types": ["vite/client"]
  },
  "exclude": [
    ".vite/",
    "dist/"
  ]
}
