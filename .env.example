# Example environment variables file
# Copy this to .env for local development
# Do not commit .env to version control

# Anthropic API Key
ANTHROPIC_API_KEY=

# OpenAI API Key
OPENAI_API_KEY=

# S3 Storage Settings
S3_ACCESS_KEY_ID=
S3_SECRET_ACCESS_KEY=
S3_REGION=
S3_BUCKET_NAME=
# Optional
# For R2, the endpoint is https://<account-id>.r2.cloudflarestorage.com
# For S3, the endpoint is automatically generated
S3_ENDPOINT=
S3_CUSTOM_DOMAIN=

# MCP Store Configuration (CoreSpeed MCP Registry)
# Optional: Override the MCP Store base URL (e.g., for private registries)
# Default: https://api.corespeed.ai
MCP_STORE_BASE_URL=